{
    "name": "naoray/laravel-localizer",
    "description": "Tool to manage multiple language app",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel",
        "localizer",
        "language"
    ],
    "authors": [
        {
            "name": "Krishan Koenig",
            "email": "krishan.koenig@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.7.0|~5.8.0",
        "illuminate/routing": "~5.7.0|~5.8.0",
        "codezero/laravel-localizer": "^1.0"
    },
    "require-dev": {
      "orchestra/testbench": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "Naoray\\LaravelLocalizer\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Naoray\\LaravelLocalizer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Naoray\\LaravelLocalizer\\LaravelLocalizerServiceProvider"
            ]
        }
    }
}
